<h1><%= @user.first_name%> <%= @user.last_name %>'s playlist</h1>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Artist</th>
      <th scope="col">Title</th>
      <th scope="col">Times Added</th>
    </tr>
  </thead>
  <tbody <%= @songs.each do |song| %>>
    <tr>
        <% if song.user_playlists.user_id = current_user.id %>
        <td><%= song.artist %></td>
        <td><a href="/songs/<%= song.id %>"><%= song.title %></a></td>
        <td>
          <% song.user_playlist.user_id.count %>
        </td>
        <% end %>
      <% end %>
    </tr>
  </tbody>
</table>
