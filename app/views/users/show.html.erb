<h1><%= @user.first_name%> <%= @user.last_name %>'s playlist</h1>
<a type="button" href="/songs">To Songs</a>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Artist</th>
      <th scope="col">Title</th>
      <th scope="col">Times Added</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.artist %></td>
      <td><a href="/songs/<%= song.id %>"><%= song.title %></a></td>
      <td><%= song.user_playlists.count %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
